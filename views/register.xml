                <main>
                    <div class="bg_color_2">
                        <div class="container margin_60_35">
                            <div id="register">
                                <h1>Please register to Findoctor!</h1>
                                <div class="row justify-content-center">
                                    <div class="col-md-5">
                                        <form>
                                            <div class="box_form">
                                                <div class="form-group">
                                                    <label>Name</label>
                                                    <input type="text" class="form-control" placeholder="Your name"/>
